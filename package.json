{
    "name": "midi",
    "module": "index.ts",
    "type": "module",
    "files": [
        "src/index.ts",
        "assets/*"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "npm run",
        "tailwindcss": "npx tailwindcss -i ./styles.css -o ./dist/output.css --watch",
        "dev": "bun build --watch --outdir=dist/ ./src/index.ts --entry-naming '[dir]/[name].[ext]'",
        "watch_wasm": "npm-run-all --parallel build-ts tailwindcss cp-wasm",
        "watch": "npm-run-all --parallel build-ts tailwindcss",
        "cp-wasm": "cp ./wasm-eq/pkg/wasm_eq_bg.wasm ./dist",
        "build-ts": "./build.js"
    },
    "devDependencies": {
        "bun-types": "latest",
        "esbuild": "^0.21.4",
        "esbuild-plugin-copy": "^2.1.1",
        "tailwindcss": "^3.4.3"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5",
        "wasm-eq": "file:./wasm-eq/pkg/"
    }
}
